{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "login",
    "password",
    "status"
  ],
  "properties": {
    "login": {
      "type": "string"
    },
    "password": {
      "type": "string"
    },
    "status": {
      "enum": ["active", "blocked"]
    }
  }
}